          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="Group2SummaryTableAdapter" GeneratorDataComponentClassName="Group2SummaryTableAdapter" Name="Group2Summary" UserDataComponentName="Group2SummaryTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="cppConnectionString (Web.config)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT ag.gr2Code, ag.gr2CatID, ag.AuthorizedBudget, ag.ForecastedBudgetChange, ac.ID, ac.OrganizationID, ac.CategoryID, ac.CategoryDescription, ac.SubCategoryID, ac.SubCategoryDescription, ac.Phase, ac.CreatedDate, ac.UpdatedDate, 
                  ac.CreatedBy, ac.UpdatedBy
FROM     (SELECT LEFT(SubCategoryID, 1) AS gr2Code, MIN(CategoryID) AS gr2CatID, SUM(CurrentAuthorizedBudget) AS AuthorizedBudget, SUM(ForecastAmount) AS ForecastedBudgetChange
                  FROM      (SELECT c.SubCategoryID, c.CategoryID, i_1.CurrentAuthorizedBudget, j_1.ForecastAmount
                                     FROM      activity_category c LEFT OUTER JOIN
                                                           (SELECT j.CategoryDescription, j.CategoryID, j.SubCategoryDescription, j.SubCategoryID, SUM(IFNULL(i.FTECost, 0) + IFNULL(i.LumpsumCost, 0) + IFNULL(i.UnitCost, 0)) AS CurrentAuthorizedBudget
                                                            FROM      activity_category j LEFT OUTER JOIN
                                                                              activity i ON i.BudgetSubCategory = j.SubCategoryDescription AND i.BudgetCategory = j.CategoryDescription AND i.ProjectID = :ProjectID AND i.TrendNumber &lt;= :TrendNumber
                                                            GROUP BY j.SubCategoryID, j.SubCategoryDescription) i_1 ON i_1.SubCategoryID = c.SubCategoryID LEFT OUTER JOIN
                                                           (SELECT j.CategoryDescription, j.CategoryID, j.SubCategoryDescription, j.SubCategoryID, SUM(IFNULL(i.FTECost, 0) + IFNULL(i.LumpsumCost, 0) + IFNULL(i.UnitCost, 0)) AS ForecastAmount
                                                            FROM      activity_category j LEFT OUTER JOIN
                                                                              activity i ON i.BudgetSubCategory = j.SubCategoryDescription AND i.BudgetCategory = j.CategoryDescription AND i.ProjectID = :ProjectID AND i.TrendNumber &lt;= :TrendNumber
                                                            GROUP BY j.SubCategoryID, j.SubCategoryDescription) j_1 ON j_1.SubCategoryID = c.SubCategoryID) m
                  GROUP BY LEFT(SubCategoryID, 1)) ag INNER JOIN
                  activity_category ac ON ag.gr2CatID = ac.SubCategoryID</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="ProjectID" ColumnName="" DataSourceName="" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="ProjectID" Precision="0" Scale="0" Size="0" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
