          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="ForecastTrendTableAdapter" GeneratorDataComponentClassName="ForecastTrendTableAdapter" Name="ForecastTrend" UserDataComponentName="ForecastTrendTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="cppConnectionString (Web.config)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT c.ID, c.OrganizationID, c.CategoryID, c.CategoryDescription, c.SubCategoryID, c.SubCategoryDescription, i_1.CurrentAuthorizedBudget, j_1.ForecastAmount
FROM     activity_category c LEFT OUTER JOIN
                      (SELECT actCat.ID, actCat.OrganizationID, act.BudgetCategory, act.BudgetSubCategory, SUM(act.FTECost + act.LumpsumCost + act.UnitCost + act.OdcCost) AS CurrentAuthorizedBudget
                       FROM      project proj INNER JOIN
                                         program prg ON prg.ProgramID = proj.ProgramID INNER JOIN
                                         trend tr ON tr.ProjectID = proj.ProjectID INNER JOIN
                                         activity act ON act.TrendID = tr.TrendID INNER JOIN
                                         activity_category actCat ON actCat.SubCategoryDescription = act.BudgetSubCategory AND actCat.CategoryDescription = act.BudgetCategory AND actCat.OrganizationID = prg.OrganizationID
                       WHERE   (proj.ProjectID = :ProjectID) AND (tr.TrendNumber &lt;&gt; '1000') AND (act.TrendNumber &lt;&gt; '1000') AND (tr.TrendStatusID = 1)
                       GROUP BY act.BudgetCategory, act.BudgetSubCategory) i_1 ON i_1.ID = c.ID LEFT OUTER JOIN
                      (SELECT actCat.ID, actCat.OrganizationID, act.BudgetCategory, act.BudgetSubCategory, SUM(act.FTECost + act.LumpsumCost + act.UnitCost + act.OdcCost) AS ForecastAmount
                       FROM      project proj INNER JOIN
                                         program prg ON prg.ProgramID = proj.ProgramID INNER JOIN
                                         trend tr ON tr.ProjectID = proj.ProjectID INNER JOIN
                                         activity act ON act.TrendID = tr.TrendID INNER JOIN
                                         activity_category actCat ON actCat.SubCategoryDescription = act.BudgetSubCategory AND actCat.CategoryDescription = act.BudgetCategory AND actCat.OrganizationID = prg.OrganizationID
                       WHERE   (proj.ProjectID = :ProjectID) AND (tr.TrendNumber &lt;&gt; '1000') AND (act.TrendNumber &lt;&gt; '1000')
                       GROUP BY act.PhaseCode, act.BudgetCategory, act.BudgetSubCategory) j_1 ON j_1.ID = c.ID
WHERE  (c.OrganizationID =
                      (SELECT OrganizationID
                       FROM      program
                       WHERE   (ProgramID =
                                             (SELECT ProgramID
                                              FROM      Project
                                              WHERE   (ProjectID = :ProjectID)))))
ORDER BY c.SubCategoryID</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="ProjectID" ColumnName="" DataSourceName="" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="ProjectID" Precision="0" Scale="0" Size="0" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
