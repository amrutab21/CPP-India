                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT tre.TrendID, tre.TrendNumber, tre.ProjectID, tre.TrendDescription, tre.TrendJustification, tre.TrendImpact, proj.ProjectName, ProjectManager.Name AS ProjectManagerName, Director.Name AS DirectorName, 
                  Scheduler.Name AS SchedulerName, VicePresident.Name AS VicePresidentName, FinancialAnalyst.Name AS FinancialAnalystName, CapitalProjectAssistant.Name AS CapitalProjectAssistantName
FROM     trend tre INNER JOIN
                  project proj ON tre.ProjectID = proj.ProjectID INNER JOIN
                  employee ProjectManager ON proj.ProjectManagerID = ProjectManager.ID INNER JOIN
                  employee Director ON proj.DirectorID = Director.ID INNER JOIN
                  employee Scheduler ON proj.SchedulerID = Scheduler.ID INNER JOIN
                  employee VicePresident ON proj.VicePresidentID = VicePresident.ID INNER JOIN
                  employee FinancialAnalyst ON proj.FinancialAnalystID = FinancialAnalyst.ID INNER JOIN
                  employee CapitalProjectAssistant ON proj.CapitalProjectAssistantID = CapitalProjectAssistant.ID
WHERE  (tre.ProjectID = :ProjectID) AND (tre.TrendNumber = :TrendNumber)</CommandText>
