          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="PhaseDateDetailsTableAdapter" GeneratorDataComponentClassName="PhaseDateDetailsTableAdapter" Name="PhaseDateDetails" UserDataComponentName="PhaseDateDetailsTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="cppConnectionString (Web.config)" DbObjectName="cpp.phase_lookup" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT p.ActivityPhaseCode AS PhaseID, p.PhaseDescription AS PhaseTitle, MAX(ps.ActivityStartDate) AS StartDate, MAX(ps.ActivityEndDate) AS EndDate, MAX(psp.ActivityStartDate) AS PriorStartDate, MAX(psp.ActivityEndDate) 
                  AS PriorEndDate
FROM     phase_lookup p LEFT OUTER JOIN
                  activity ps ON ps.PhaseCode = p.ActivityPhaseCode AND ps.ProjectID = :ProjectID AND ps.TrendNumber = :TrendNumber LEFT OUTER JOIN
                  activity psp ON psp.PhaseCode = p.ActivityPhaseCode AND psp.ProjectID = :ProjectID AND psp.TrendNumber =
                      (SELECT MAX(TrendNumber) AS EXPR1
                       FROM      trend
                       WHERE   (ProjectID = :ProjectID) AND (TrendNumber &lt; :TrendNumber) AND (TrendStatusID = 1))
GROUP BY p.PhaseID
ORDER BY PhaseID</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="ProjectID" ColumnName="ProjectID" DataSourceName="cpp.activity" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="ProjectID" Precision="0" ProviderType="Int" Scale="0" Size="0" SourceColumn="ProjectID" SourceColumnNullMapping="false" SourceVersion="Current" />
                      <Parameter AllowDbNull="false" AutogeneratedName="TrendNumber" ColumnName="TrendNumber" DataSourceName="cpp.activity" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="TrendNumber" Precision="0" ProviderType="Int" Scale="0" Size="0" SourceColumn="TrendNumber" SourceColumnNullMapping="false" SourceVersion="Current" />
